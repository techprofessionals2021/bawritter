{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "artesaos/seotools": "^1.2.0",
        "braintree/braintree_php": "^5.0",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^3.0.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "jackiedo/dotenv-editor": "^1.1",
        "junaidnasir/larainvite": "^6.0",
        "laravel/framework": "^9.0",
        "laravel/legacy-factories": "^1.4",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "mews/purifier": "^3.2",
        "paypal/paypal-checkout-sdk": "^1.0",
        "spatie/laravel-activitylog": "^4.7.3",
        "spatie/laravel-cookie-consent": "^3.2.4",
        "spatie/laravel-permission": "^6.3.0",
        "stripe/stripe-php": "^7.37",
        "symfony/http-client": "^6.4",
        "symfony/mailgun-mailer": "^6.4",
        "symfony/postmark-mailer": "^6.4",
        "yajra/laravel-datatables-oracle": "^9.9"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^1.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "database/factories",
            "database/seeders"
        ],
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
