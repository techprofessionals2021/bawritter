{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.4.0",
        "braintree/braintree_php": "^5.0",
        "doctrine/dbal": "^3.0",
        "jackiedo/dotenv-editor": "^2.1",
        "junaidnasir/larainvite": "^6.0",
        "mews/purifier": "^3.4",
        "paypal/paypal-checkout-sdk": "^1.0",
        "spatie/laravel-activitylog": "^4.7.0",
        "spatie/laravel-cookie-consent": "^3.2.4",
        "spatie/laravel-permission": "^6.3.0",
        "stripe/stripe-php": "^13.10",
        "symfony/http-client": "^6.4.3",
        "symfony/mailgun-mailer": "^6.4.3",
        "symfony/postmark-mailer": "^6.4.3",
        "yajra/laravel-datatables-oracle": "^10.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.6.7",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.4.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "database/factories",
            "database/seeders"
        ],
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}